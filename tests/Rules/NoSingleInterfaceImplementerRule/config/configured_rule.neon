rules:
    - Symplify\PHPStanRules\Rules\NoSingleInterfaceImplementerRule

services:
    -
        class: Symplify\PHPStanRules\Collector\InterfaceCollector
        tags:
            - phpstan.collector

    -
        class: Symplify\PHPStanRules\Collector\ImplementedInterfaceCollector
        tags:
            - phpstan.collector
